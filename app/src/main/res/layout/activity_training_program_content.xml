<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="fossen.power.training_program_library.TrainingProgramContentActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar_tpc"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="?attr/colorPrimary"
        android:elevation="4dp"
        android:theme="@style/Toolbar"
        app:title="@string/title_training_program" />

    <ExpandableListView
        android:id="@+id/explist_tpc"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:childDivider="?android:attr/windowBackground"
        android:divider="?android:attr/windowBackground"
        android:groupIndicator="@null" />

</LinearLayout>
